<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.079"
            android:layout_height="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.53"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="87dp"
            motion:layout_constraintTop_toBottomOf="@+id/show_list_button"
            android:id="@+id/button" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="304dp"
            android:id="@+id/editTextTextPersonName3"
            motion:layout_constraintBottom_toTopOf="@+id/editTextTextPassword"
            android:layout_marginBottom="56dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintVertical_bias="0.883"
            motion:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/editTextTextPassword"
            motion:layout_constraintBottom_toBottomOf="@+id/show_list_button"
            android:layout_marginBottom="43dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.098"
            android:layout_height="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.532"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="87dp"
            motion:layout_constraintTop_toBottomOf="@+id/show_list_button"
            android:id="@+id/navToRegister" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="304dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/password"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/user_name"
            motion:layout_constraintVertical_bias="0.109" />
        <Constraint
            motion:layout_editor_absoluteY="0dp"
            motion:layout_editor_absoluteX="0dp"
            android:id="@+id/linearLayout" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>